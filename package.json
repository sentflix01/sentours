{
  "name": "sentours",
  "version": "1.0.0",
  "description": "learning node, express and mongoDB",
  "main": "app.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch public/js/index.js --out-dir public/js --out-file bundle.js --public-url /js/",
    "build:js": "parcel build public/js/index.js --out-dir public/js --out-file bundle.js --public-url /js/ --no-content-hash"
  },
  "repository": {
    "type": "git",
    "url": "git https://github.com/sentflix01/sentours.git"
  },
  "author": "Sintayehu Mulugeta",
  "license": "ISC",
  "overrides": {
    "form-data": "^4.0.0",
    "tough-cookie": "^4.1.3",
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "braces": "^3.0.3",
    "micromatch": "^4.0.8",
    "node-forge": "^1.3.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "mongoose": "^8.17.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.4",
    "nodemon": "^3.1.10",
    "pug": "^3.0.3",
    "sharp": "^0.34.3",
    "slugify": "^1.6.6",
    "stripe": "^18.5.0",
    "validator": "^13.15.15",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^3.4.0",
    "@types/jsonwebtoken": "^9.0.10",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "parcel-bundler": "^1.12.5",
    "prettier": "^3.6.2",
    "prettier-plugin-pug": "^1.0.0-alpha.8"
  },
  "targets": {
    "default": {
      "distDir": "./public/js",
      "fileName": "bundle.js",
      "sourceMap": false,
      "publicUrl": "/js/"
    }
  }
}
